
@{
    ViewBag.Title = "T13010";
}

<script src="~/Apps/Accounts/Setup/T13010Controller.js"></script>
<script src="~/Apps/Service/Service.js"></script>
<style>
    .selected {
        background-color: #80c8e4ba;
        /* color: white;*/
        font-weight: bold;
    }
</style>
<div class="" ng-app="Textile" ng-controller="T13010Controller">
    @*header*@
    <div ng-show="loading" style="position: absolute;padding-left: 48%; margin-top:60px"><img src="~/Images/working1.gif" style="height: 50px;"></div>
    <div class="header">
        <span href="#default" class="logo">Supplier Setup</span>
        <div class="header-right">
            <a><span title="Save" ng-click="Save_Click()" class="glyphicon glyphicon-floppy-disk"></span></a>
            <a><span title="Clear" ng-click="Clear_Click()" class="glyphicon glyphicon-erase"></span></a>
            <a><span title="Print" ng-click="Print_Click()" class="glyphicon glyphicon-print"></span></a>
            <a><span title="Delete" ng-click="Delet_Click()" class="glyphicon glyphicon-trash"></span></a>
        </div>
    </div>
    @*===========*@
    <div style="margin-left:0px; margin-top:10px" class="row col-lg-12 col-md-12 col-sm-12">
        <table class="inputTable center" style=" width: 90%;">
            <tr>
                <td style=" width: 10%;">
                    <label class="textlevel">Code</label>
                </td>
                <td style="width: 10%; text-align: left; ">
                    <input id="txtCode" type="text" ng-model="obj.T13010.T_SUPPLIER_CODE" class="form-control" readonly />
                </td>
                <td style=" width: 10%;">
                    <label id="lblSupName" class="textlevel">Sup Name</label>
                </td>
                <td style="width: 20%; text-align: left; ">
                    <input type="text" id="txtSupName" ng-model="obj.T13010.T_SUPPLIER_NAME" class="form-control" />
                </td>
                <td style=" width: 10%;">
                    <label id="lblMobile" class="textlevel"> Mobile</label>
                </td>
                <td style="width: 20%; text-align: left; ">
                    <input type="text" id="txtMobile" ng-model="obj.T13010.T_MOBILE_NO" class="form-control" maxlength="11" />
                </td>
                <td style=" width: 10%;">
                    <label id="lblType" class="textlevel">Type</label>
                </td>
                <td style="width: 10%; text-align: left; ">
                    <input type="text" id="txtType" ng-model="obj.T13010.T_SUPPLIER_TYPE" class="form-control" />
                </td>

            </tr>
            <tr>
                <td style=" width: 10%;">
                    <label id="lblVatNo" class="textlevel"> Vat No</label>
                </td>
                <td style="width: 10%; text-align: left; ">
                    <input type="text" id="txtVatNo" ng-model="obj.T13010.T_VAT_NO" class="form-control" maxlength="11" />
                </td>
                <td style=" width: 10%;">
                    <label id="lblTln" class="textlevel">TIN</label>
                </td>
                <td style="width: 20%; text-align: left; ">
                    <input type="text" id="txtTln" ng-model="obj.T13010.T_TIN_NO" class="form-control" maxlength="15" />
                </td>

                <td style=" width: 10%;">
                    <label id="lblNidNo" class="textlevel"> NID No</label>
                </td>
                <td colspan="3" style="width: 40%; text-align: left; ">
                    <input id="txtNidNo" type="text" style="min-width:100%;max-width:100%"  ng-model="obj.T13010.T_NID_NO" class="form-control" />
                </td>
            </tr>
            <tr>
                <td style=" width: 10%;">
                    <label id="lblAcntName" class="textlevel"> Acnt Name</label>
                </td>
                <td style="width: 10%; text-align: left; ">
                    <input id="txtAcntName" type="text" style="min-width:100%;max-width:100%"  ng-model="obj.T13010.T_ACCOUNT_NAME" class="form-control" />
                </td>
                <td style=" width: 10%;">
                    <label class="textlevel"> Account No</label>
                </td>
                <td style="width: 20%; text-align: left; ">
                    <input type="text" style="min-width:100%;max-width:100%" id="txtAccount" ng-model="obj.T13010.T_ACCOUNT_NO" class="form-control" />
                </td>
                <td style=" width: 10%;">
                    <label class="textlevel"> Bank Name</label>
                </td>
                <td style="width: 20%; text-align: left; ">
                    <input type="text" style="min-width:100%;max-width:100%" id="txtBankName" ng-model="obj.T13010.T_BANK_NAME" class="form-control" />
                </td>
                <td style=" width: 10%;">
                    <label class="textlevel"> Branch</label>
                </td>
                <td style="width: 10%; text-align: left; ">
                    <input type="text" style="min-width:100%;max-width:100%" id="txtBranch" ng-model="obj.T13010.T_BRANCH" class="form-control" />
                </td>
            </tr>
        </table>
        <hr style="margin-top: 5px !important; margin-bottom: 5px !important; border-top: 2px solid #076c93b0; " />
        <div style="width: 98%;margin:auto">
            <table class="inputTable center" style=" width: 100%;">
                <tr><td style="text-align: -webkit-right;"> <input type="text" style="width:200px" ng-model="Search" placeholder="Search....." class="form-control" /></td></tr>
            </table>
            <table id="tblGrid" class="tTable center" style=" width: 100%;">
                <thead>
                    <tr style="color: black;background: #afadad" class="summary-table-header">
                        <th style="width: 5%;">Code</th>
                        <th style="width: 15%;">Name</th>
                        <th style="width: 10%;">Mobile</th>
                        <th style="width: 10%;">TIN NO</th>
                        @*<th style="width: 5%;">Vat No</th>*@
                        <th style="width: 10%;">NID No</th>
                        <th style="width: 10%;">Account Name</th>
                        <th style="width: 10%;">Account No</th>
                        <th style="width: 10%;">Bank Name</th>
                        <th style="width: 10%;">Branch</th>
                        <th style="width: 10%;">Type</th>
                    </tr>
                </thead>
                <tbody>
                    <tr dir-paginate="Z in obj.griDataList|filter:Search|itemsPerPage:10" $index pagination-id="details" ng-class="{'selected':$index == selectedRow}" ng-click="selectedtRow($index,Z)" style="cursor:pointer">
                        <td style="width: 5%;">{{Z.T_SUPPLIER_CODE}}</td>
                        <td style="width: 15%; text-align: left; padding-left: 10px;">{{Z.T_SUPPLIER_NAME}}</td>
                        <td style="width: 10%; text-align: left; padding-left: 10px;">{{Z.T_MOBILE_NO}}</td>
                        <td style="width: 10%; text-align: left; padding-left: 10px;">{{Z.T_TIN_NO}}</td>
                        @*<td style="width: 5%; text-align: left; padding-left: 10px;">{{Z.T_VAT_NO}}</td>*@
                        <td style="width: 10%; text-align: left; padding-left: 10px;">{{Z.T_NID_NO}}</td>
                        <td style="width: 10%; text-align: left; padding-left: 10px;">{{Z.T_ACCOUNT_NAME}}</td>
                        <td style="width: 10%; text-align: left; padding-left: 10px;">{{Z.T_ACCOUNT_NO}}</td>
                        <td style="width: 10px; text-align: left; padding-left: 10px;">{{Z.T_BANK_NAME}}</td>
                        <td style="width: 10%; text-align: left; padding-left: 10px; white-space: nowrap;">{{Z.T_BRANCH}}</td>
                        <td style="width: 10%; text-align: left; padding-left: 10px;">{{Z.T_SUPPLIER_TYPE}}</td>

                    </tr>
                </tbody>
            </table>
            <dir-pagination-controls pagination-id="details" max-size="5"
                                     direction-links="true"
                                     boundary-links="true">
            </dir-pagination-controls>

        </div>
    </div>

</div>




